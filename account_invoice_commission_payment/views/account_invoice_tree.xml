<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="invoice_tree_commission" model="ir.ui.view">
        <field name="name">account.invoice.tree.commission</field>
        <field name="model">account.invoice</field>
        <field name="priority" eval="99" />
        <field name="arch" type="xml">

            <tree string="Invoice">
                <field name="partner_id"/>
                <field name="date_invoice"/>
                <field name="number"/>
                <field name="date_due"/>
                <field name="amount_untaxed_invoice_signed" string="Tax Excluded" sum="Total"/>
                <field name="amount_tax_signed" string="Tax" sum="Total"/>
                <field name="amount_total_signed" string="Total" sum="Total"/>
                <field name="purchase_price" string="Commission" sum="Commission"/>
                <field name="currency_id" invisible="1"/>
                <field name="company_currency_id" invisible="1"/>
                <field name="state"/>
            </tree>

        </field>
    </record>

</odoo>
